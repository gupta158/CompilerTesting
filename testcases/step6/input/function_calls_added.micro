PROGRAM functions
BEGIN

	STRING space := " ";
	STRING eol := "\n";


	FUNCTION INT FOO (INT n)
	BEGIN
		INT ret1;
		IF (n > 4)
			ret1 := FOO(n-1) + 1 + 2 + 4 + 5 + 6 + 8 + 9;
		FI
		RETURN ret1;
	END
	
	
	FUNCTION INT main ()
	BEGIN
		INT i, result;

		result := FOO(1) + FOO(2) + FOO(3) + FOO(4) + FOO(5);
		i := FOO(1) + FOO(2) + 4 + FOO(3) * FOO(FOO(3));
		WRITE(result, eol, space, space, space, space);
		WRITE(i);

		RETURN 0;
	END

END	
